#include <stdio.h>
#include <math.h>
#include <limits.h>
#include <float.h>
#include <iostream>
#include <windows.h>
using namespace std;
int main()
{
	setlocale (0,"");
	printf("signed char принимает значения от %d до %d\n", CHAR_MIN, CHAR_MAX);	
	printf("unsigned char принимает значения от 0 до %d\n", UCHAR_MAX);
	printf("signed int принимает значения от %d до %d\n", INT_MIN, INT_MAX);
	printf("unsigned int принимает значения от 0 до %u\n", UINT_MAX); 
	printf("signed short принимает значения от %d до %d\n", SHRT_MIN, SHRT_MAX);
	printf("unsigned short принимает значения от 0 до %d\n", USHRT_MAX);
	printf("signed long принимает значения от %u до %ld\n", LONG_MIN, LONG_MAX);
	printf("unsigned long принимает значения от 0 до %lu\n", ULONG_MAX);
Sleep (1000000000);
return 0;
}
